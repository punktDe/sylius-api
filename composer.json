{
    "description": "Sylius Shop Admin API Client for the Flow Framework",
    "type": "neos-package",
    "name": "punktde/sylius-api",
    "license": "MIT",
    "keywords": [
        "Sylius",
        "Shop",
        "Api",
        "Neos",
        "Flow Framework",
        "API Platform"
    ],
    "require": {
        "neos/flow": "^5.0 || ^6.0 || ^7.0 || ^8.0",
        "symfony/serializer": "^4.0",
        "symfony/property-access": "^4.1",
        "eljam/guzzle-jwt-middleware": "^0.7 || ^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "PunktDe\\Sylius\\Api\\": "Classes/"
        }
    },
    "extra": {
        "neos": {
            "package-key": "PunktDe.Sylius.Api"
        }
    }
}
